<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp1.1;net452</TargetFrameworks>
    <OutputType>exe</OutputType>
    <OutputTypeEx>exe</OutputTypeEx>
  </PropertyGroup>

  <PropertyGroup>
    <Description />
    <Company />
    <Copyright>Copyright ©  2012</Copyright>
    <Authors />
    <ApplicationIcon />
